<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yzb.pad.dao.BunkerDao">
    <sql id="Base_Column_List">
        F_Bunker as bunker, F_Raw as raw, F_Batch as batch, F_Weight as weight
    </sql>
    <insert id="save">
      INSERT INTO T_AddRaw
      (F_Bunker,F_Raw ,F_Batch,F_Weight)
      VALUES
      (#{bunker},#{raw} ,#{batch},#{weight})
    </insert>
    <update id="update">
        UPDATE T_AddRaw
        SET
        F_Raw =#{raw},F_Batch=#{batch},F_Weight=#{weight}
        WHERE
        F_Bunker=#{bunker}
    </update>
    <select id="selectByPrimaryKey" resultType="com.yzb.pad.pojo.Bunker" parameterType="string">
        select
        <include refid="Base_Column_List"/>
        from T_AddRaw
        where F_Bunker = #{bunker}
    </select>

    <select id="listBunkerByPage" resultType="com.yzb.pad.pojo.Bunker">
        select
        <include refid="Base_Column_List"/>
        from T_AddRaw
    </select>

</mapper>